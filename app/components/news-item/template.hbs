<div class="layout-row layout-xs-column">
    {{#paper-card as |card|}}
      {{#card.title as |title|}}
        {{#title.text as |text|}}
          {{#text.headline}}{{new.title}}{{/text.headline}}
          {{#text.subhead}}
          <div>
            <p>{{new.date}}</p>
            <p>{{new.body}}</p>
          </div>
          {{/text.subhead}}
        {{/title.text}}
          {{card.image class="" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTlcyAu-8qd_sQy5AGQiY17l7XyXZ49i07D-19PoOH7qVvhHLK2" alt="Washed Out"}}
      {{/card.title}}
      {{#card.actions}}
        {{#paper-button}}Read More{{/paper-button}}
      {{/card.actions}}
    {{/paper-card}}
</div>
