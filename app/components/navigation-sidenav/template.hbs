          {{#link-to "login.settings" class="navigation-sidenav__item-row"}}
            {{paper-icon "library-add" class="navigation-sidenav__item-icon"}}
            <span class="navigation-sidenav__item-text">Settings</span>
          {{/link-to}}
        </div>
        {{paper-divider}}
        <div class="navigation-sidenav__item layout-row">
          <a {{action 'invalidateSession'}} class="navigation-sidenav__item-row">
            {{paper-icon "account-circle" class="navigation-sidenav__item-icon"}}
            <span class="navigation-sidenav__item-text">Logout</span>
          </a>
        </div>
      {{else}}
        <div class="navigation-sidenav__item layout-row">
          {{#link-to "login" class="navigation-sidenav__item-row"}}
            {{paper-icon "account-circle" class="navigation-sidenav__item-icon"}}
            <span class="navigation-sidenav__item-text">Login</span>
