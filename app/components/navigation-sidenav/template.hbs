  {{#paper-sidenav
    class="navigation-sidenav md-whiteframe-z2"
    name="left"
    position="left"
    open=rightSideBarOpen
    lockedOpen=false
    onToggle=(action (mut rightSideBarOpen))}}
    {{#paper-toolbar class="navigation-sidenav__logo layout-row layout-align-center-center" as |toolbar|}}
      <img src="/assets/images/logos.gif" alt="gemSeek/cQuest">
    {{/paper-toolbar}}
    {{paper-divider}}
    {{#paper-content padding=true}}
      <div class="navigation-sidenav__item layout-row">
        {{#link-to "home" class="navigation-sidenav__item-row"}}
          {{paper-icon "home" class="navigation-sidenav__item-icon"}}
          <span class="navigation-sidenav__item-text">Home</span>
        {{/link-to}}
      </div>
      {{paper-divider}}
