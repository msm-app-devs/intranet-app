      </div>
      <div class="navigation-sidenav__item layout-row">
        {{#link-to "hr-folder.hr-documents" class="navigation-sidenav__item-row"}}
          <img src="/assets/images/documents.svg">
          <span class="navigation-sidenav__item-text">Documents</span>
        {{/link-to}}
      </div>

      <div class="navigation-sidenav__item layout-row">
        {{#link-to "hr-folder.hr-recruitment" class="navigation-sidenav__item-row"}}
          <img src="/assets/images/recruitment.svg">
