          {{paper-icon "folder-shared" class="navigation-sidenav__item-icon"}}
          <span class="navigation-sidenav__item-text">Benefits</span>
        {{/link-to}}
      </div> --}}
      {{paper-divider}}
      <div class="navigation-sidenav__item layout-row">
        {{#link-to "hr-folder.hr-newsletter" class="navigation-sidenav__item-row"}}
          <img src="/assets/images/newsletter.svg">
