            {{paper-icon "settings" class="navigation-sidenav__item-icon"}}
            <span class="navigation-sidenav__item-text">Edit News</span>
          {{/link-to}}
        </div>
        <div class="navigation-sidenav__item layout-row">
          {{#link-to "login.employees" class="navigation-sidenav__item-row"}}
